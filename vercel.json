{
    "version": 2,
    "builds": [
      { "start:dev": "nodemon --watch 'src/' --exec 'ts-node src/index.ts' -e ts",
      "start": "node build/index.js",
      "build": "tsc --project tsconfig.json"
        
       
      }
    ],
    "routes": [
      {
        "src": "(.*)",
        "dest": "src/index.ts"
      }
    ]
  }